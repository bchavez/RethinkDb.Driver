  "authors": [
    "Brian Chavez"
  ],
  "tags": [
    ""
  ],
  "projectUrl": "https://github.com/bchavez/RethinkDb.Driver",
  "licenseUrl": "https://raw.githubusercontent.com/bchavez/RethinkDb.Driver/master/LICENSE",
  "configurations": {
    "Debug": {
      "compilationOptions": {}
    },
    "Release": {
      "compilationOptions": {
        "optimize": true
      }
    }
  },
